#!/usr/bin/env ruby
require_relative("lib/downloader_base.rb")

class FromUrlDownloader < DownloaderBase
  EXECUTABLE = "curl -O"
  DEFAULTS = %w(
    http://upload.wikimedia.org/wikipedia/commons/thumb/e/ed/Parallel_postulate_en.svg/525px-Parallel_postulate_en.svg.png
    http://www.gnu.org/software/parallel/logo-gray+black300.png
  )

  def initialize
    super(EXECUTABLE, DEFAULTS, ARGV)
  end
end

if __FILE__ == $0
  FromUrlDownloader.new.run
end

