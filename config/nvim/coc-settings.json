{
  "coc.preferences.formatOnSaveFiletypes": [
    "bash",
    "bib",
    "conf",
    "css",
    "gitconfig",
    "go",
    "javascript",
    "json",
    "neosnippet",
    "r",
    "rhelp",
    "rmd",
    "scala",
    "scss",
    "sh",
    "sql",
    "tmux",
    "typescript",
    "vim",
    "zsh"
  ],
  "jest.detectLeaks": true,
  "jest.terminalPosition": "right",
  "jest.watchman": true,
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": true,
  "pyright.organizeimports.provider": "isort",
  "pyright.testing.provider": "pytest",
  "sql.formatOptions": {
    "language": "snowflake",
    "keywordCase": "upper",
    "dataTypeCase": "upper",
    "expressionWidth": 128
  }
}
