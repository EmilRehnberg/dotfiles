
snippet readtsv
abbr    readr::read_tsv
alias   read_tsv
options word
  readr::read_tsv("${1:#:path}") %>%

snippet readcsv
abbr    readr::read_csv
alias   read_csv
options word
  readr::read_csv("${1:#:path}") %>%

snippet readdelim
abbr    readr::read_delim
alias   read_delim
options word
  readr::read_delim("${1:#:path}", delim = "${2:,}") %>%
